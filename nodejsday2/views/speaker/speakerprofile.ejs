    <title>speaker</title>

</head>

<body>
  <%- include("../head.ejs") %>



  <% for( let index = 0; index < error.length; index++ ) { %>
                 
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
      <strong>YOU HV ERR!</strong> <lI class="bg-danger bg-white"><%=error[index]%></lI>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <% } %>
<br>
<br>

<div class="container "></div>


<table class="table table-striped table-success">
  <thead>
    <tr>
      <th scope="col">the speaker name</th>
      <th scope="col"><%=fullname%></th>
     
    </tr>

    <tr>
      <th scope="col">the speaker username:</th>
      <th scope="col"><%=username%></th>
     
    </tr>
<!--  -->
    <tr>
      <th scope="col">the speaker age :</th>
      <th scope="col"><%=age%> </th>
     
    </tr>

    <tr>
      <th scope="col"> the address city :</th>
      <th scope="col"><%=address.city%></th>
     
    </tr>
    <tr>
      <th scope="col">the address street :</th>
      <th scope="col"><%=address.street%></th>
     
    </tr>
    <tr>
      <th scope="col">the address building :</th>
      <th scope="col"><%=address.building%></th>
     
    </tr>
  </thead>
  
</table>
<a class="btn btn-primary btn-lg btn-block" href="/speaker/update?_id=<%= _id %> ">Edit  My Profile</a>



<table class="mt-3 table table-dark">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">title</th>
      <th scope="col">date</th>
      <th scope="col">mainspeaker</th>

      <th scope="col">sup-speaker</th>
 

    </tr>
      <% if (typeof events !='undefined') { %>
    <% events.forEach(element => { %>
      <!-- <p> <%=element%></p> -->

          <tr>
              <td><%=element._id%></td>
              <td><%=element.title%></td>
              <td><%=element.date%></td>
            
              <td>
                
              
             
                <ul>
                  <h2>mainspeaker</h2>
                  <%if(element.mainspeaker){ %>
                  <li><address>fullname: <%=element.mainspeaker.fullname%> </address></li>
                 
                  <li><address>id: <%=element.mainspeaker._id%> </address></li>
             
                 <li><address>age: <%=element.mainspeaker.age%> </address></li>
                 <%}else{%>
                  <li><address>not choosed pefore  plz edit and delet mainspeaker   </address></li>
                  <%}%>
                </ul>

                
              
              </td>

              <td>
                
              <% element.otherspeaker.forEach(element => { %>
                <ul>
                  <h3>sup speaker</h3>
                  <li><address>fullname: <%=element.fullname%> </address></li>
                  <li><address>id: <%=element._id%> </address></li>
                  <li><address>age: <%=element.age%> </address></li>
                 
                </ul>
              <% }) %>
             
              

                
              
              </td>
              <td>
                <%if(element.mainspeaker){ %>
                <button><a href="/speaker/cancel?sp_id=<%=element.mainspeaker._id %>&ev_id=<%=element._id %> ">cancel</a></button>
                <% } %>
              </td>

        
             

          </tr>

                 <%}); %> 
                 <%}; %> 
  </thead>
  <tbody>

  </tbody>
</table>
<br>
<br>


             

    
</body>
</html>


